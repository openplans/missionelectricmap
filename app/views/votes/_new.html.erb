<%= form_for [supportable, supportable.votes.new], :html => { :class => "vote", "data-behavior" => "load_result_in_popup" } do |form| %>
  <button type="submit">
    <div>
      <span class="vote-label"><%= supportable.votes.count %></span>
      <%= t 'feature.vote_action' %>
    </div>
  </button>
<% end %>